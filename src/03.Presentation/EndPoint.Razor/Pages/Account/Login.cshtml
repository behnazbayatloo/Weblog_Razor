@page
@model EndPoint.Razor.Pages.Account.LoginModel

@{
    ViewData["Title"] = "ورود";
   
}
<form method="post">
    <div class="form-group">
        <label for="exampleInputEmail1">Email address</label>
        <input asp-for="Model.Email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email" required>

    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Password</label>
        <input asp-for="Model.Password" type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" required>
    </div>

    <button type="submit" class="btn btn-primary">ورود</button>
</form>
